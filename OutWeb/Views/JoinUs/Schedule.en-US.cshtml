    string cate_id = "";
    string lang_id = "";
    string lang_name = "";
    string cate_name = "";
    string ci = "";
    string c_tab = "";
    string c_tab_nav = "";
}

@section breadcrumb {
    <li><a href="~/JoinUs?langCode=@OutWeb.App_GlobalResources.Res.lang">@Res.nav_joinUs</a></li>
    <li><a href="~/JoinUs/Live?langCode=@OutWeb.App_GlobalResources.Res.lang">@Res.joinUs_joinCollege</a></li>
